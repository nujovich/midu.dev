{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layout/*": ["src/layout/*"],
      "@pages/*": ["src/pages/*"],
      "@data/*": ["src/data/*"],
      "@services/*": ["src/services/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "allowSyntheticDefaultImports": true
}
