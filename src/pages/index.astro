---
import Layout from "@layout/Layout.astro";
import Grid from "@components/Grid.astro";
import Hero from "@components/Hero.astro";
import LatestVideos from "@components/LatestVideos.astro";
import LatestReels from "@components/LatestReels.astro";
import Resources from "@components/Resources.astro";
import RecentArticles from "@components/RecentArticles.astro";
---

<Layout>
  <div class='w-full max-w-full relative'>
    <Grid />
  </div>
  <div class="relative z-10">
    <Hero />
    <LatestVideos />
    <LatestReels />
    <Resources />
    <RecentArticles />
  </div>
</Layout>
